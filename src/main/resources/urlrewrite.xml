<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <!-- 登录页 -->
    <rule>
        <from>^/login.v</from>
        <to type="forward">/static/login/login.html</to>
    </rule>

    <rule>
        <from>^/login.s</from>
        <to type="forward">/static/login/logincss/</to>
    </rule>

    <rule>
        <from>^/lib.s</from>
        <to type="forward">/static/lib/</to>
    </rule>
    <rule>
        <from>^/main.s</from>
        <to type="forward">/static/main/</to>
    </rule>
    <rule>
        <from>^/main.v</from>
        <to type="forward">/static/main/index.html</to>
    </rule>
    <rule>
        <from>^/sys.s</from>
        <to type="forward">/static/sys/</to>
    </rule>
    <rule>
        <from>^/business.s</from>
        <to type="forward">/static/business/</to>
    </rule>
    <rule>
        <from>^/form.s</from>
        <to type="forward">/static/formGeneration/</to>
    </rule>
    <rule>
        <from>^/upload.s</from>
        <to type="forward">/static/upload/</to>
    </rule>
    <rule>
    <from>^/hello.v</from>
    <to type="forward">/static/main/hello.html</to>
</rule>
</urlrewrite>
